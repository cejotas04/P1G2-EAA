function EjercicioDos(){
    
}
function EjercicioCinco(){
    var R = 5  //Usuario
    var H = 12 //Usuario
    var A = 0  //programa
    var V = 0  //programa
    var pi = 3.1416  //programa

    A = (2 * pi * R * H) + (2 * pi * (R * R))
    V = pi * (R * R) * H

    alert("El area es: " + A)
    alert("El Volumen es: " + V)
}
//Codigo de programacion - FIN
</script>
